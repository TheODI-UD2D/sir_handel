<% orgs = {
  'odi' => 'http://theodi.org/',
  'ud2d' => 'http://www.urbandata2decide.eu/',
  'london-underground' => 'http://tfl.gov.uk'
} %>

<footer class='footer'>
  <div class='container'>
    <ul class='list-inline'>
      <% orgs.each do |org, url| %>
        <li>
          <a href='<%= url %>'>
            <img src='/public/assets/<%= org %>-logo.png' />
          </a>
        </li>
      <% end %>
  </div>
</footer>
