<div class='row'>
  <% @signals.each do |key, value| %>
    <% unless I18n.t(key).match /translation missing/ %>
      <div class='col-md-4'>
        <a href="<%= SirHandel::build_url(key, request.base_url, nil) %>">
          <i class='fa fa-subway'></i>
          <%= I18n.t(key) %>
        </a>
      </div>
    <% end %>
  <% end %>
</div>
